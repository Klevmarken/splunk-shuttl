<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:forwarderConf xmlns:ns2="com.splunk.shep.server.model">
    <hdfssinklist>
        <hdfssink>
            <name>directwrite</name>
            <cluster>dev</cluster>
            <filePrefix>/splunkeventdata</filePrefix>
            <fileRollingSize>63000</fileRollingSize>
            <useAppending>false</useAppending>
        </hdfssink>
    </hdfssinklist>
    <flumesinklist>
        <flumesink>
            <name>flumewrite</name>
            <cluster>test</cluster>
            <maxEventSizeKB>64000</maxEventSizeKB>
        </flumesink>
    </flumesinklist>
</ns2:forwarderConf>
